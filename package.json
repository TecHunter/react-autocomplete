{
  "name": "react-autocomplete",
  "version": "1.8.1",
  "description": "Accessible, extensible, Autocomplete for React.js",
  "main": "./build/lib/Autocomplete.js",
  "files": [
    "build/lib/index.js",
    "build/lib/Autocomplete.js",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/reactjs/react-autocomplete.git"
  },
  "homepage": "https://github.com/reactjs/react-autocomplete",
  "bugs": "https://github.com/reactjs/react-autocomplete/issues",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "test": "jest --no-cache",
    "build": "rollup -c rollup.config.babel.js",
    "watch": "rollup -c rollup.config.babel.js -w",
    "generate-readme": "scripty",
    "gh-pages": "scripty",
    "release": "scripty",
    "build:component": "scripty",
    "build:examples": "scripty",
    "test:lint": "scripty",
    "test:jest": "scripty",
    "coverage": "scripty",
    "start": "SCRIPTY_PARALLEL=true scripty",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "authors": [
    "Ryan Florence <rpflorence@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "browserify-hmr": "^0.3.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^7.13.0",
    "exorcist": "^1.0.1",
    "jest": "^24.8.0",
    "lodash.flow": "^3.5.0",
    "lodash.map": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "react": "^16.8.6",
    "react-docgen": "^4.1.1",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "rollup": "^1.12.3",
    "rollup-jest": "^0.0.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "scripty": "^1.7.1",
    "st": "^1.2.0",
    "uglify-js": "^3.5.15",
    "watchify": "^3.8.0",
    "youemdee": "^1.0.0"
  },
  "tags": [
    "react",
    "autocomplete",
    "combobox",
    "a11y"
  ],
  "keywords": [],
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "3",
    "dom-scroll-into-view": "1.2.1",
    "jest-plugins": "^2.9.0",
    "jest-plugins-react": "^2.9.0",
    "jest-plugins-recommended": "^2.9.0",
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0-0 || ^16.0.0-0",
